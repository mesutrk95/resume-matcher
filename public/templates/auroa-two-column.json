      "h2": {
        "fontSize": 18,
        "fontWeight": "bold",
        "paddingBottom": 2
      },
      "h3": {
        "fontSize": 16,
        "fontWeight": "bold",
        "paddingBottom": 4
      },
      "h4": {
        "fontSize": 14,
        "fontWeight": "bold"
