  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed/index.ts"
  },
  "scripts": {
    "dev": "next dev -p 8998",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "prisma:update": "npx prisma generate && npx prisma db push",
    "prisma:clean": "npx prisma generate && npx prisma migrate reset && npx prisma db push",
    "prisma:seed": "npx prisma db seed",
    "postinstall": "prisma generate",
    "test": "vitest run --exclude '**/*.integration.test.{js,ts,jsx,tsx}'",
    "test:integration": "vitest run --dir lib --testNamePattern='integration'",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "resume-templates:migrate": "npx tsx ./scripts/migrate-resume.ts ./public/templates",
    "generate:resume-templates-pdf": "npx tsx scripts/generate-resume-image.tsx public/templates",
    "generate:png-logo": "npx tsx scripts/generate-logo-pngs.ts public/logos/svg/ public/logos/png/ "
  },
